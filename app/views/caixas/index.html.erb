<p id="notice"><%= notice %></p>

<h1>Caixas</h1>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th>Caixa inicial</th>
      <th>Dinheiro</th>
      <th>Saida</th>
      <th>Descricao</th>
      <th>Cheque</th>
      <th>Maquineta loja</th>
      <th>Maquineta motoboy</th>
      <th>Cappta</th>
      <th>Mundipagg</th>
      <th>Caixa final</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @caixas.each do |caixa| %>
      <tr>
        <td><%= caixa.data %></td>
        <td><%= caixa.caixa_inicial %></td>
        <td><%= caixa.dinheiro %></td>
        <td><%= caixa.saida %></td>
        <td><%= caixa.descricao %></td>
        <td><%= caixa.cheque %></td>
        <td><%= caixa.maquineta_loja %></td>
        <td><%= caixa.maquineta_motoboy %></td>
        <td><%= caixa.cappta %></td>
        <td><%= caixa.mundipagg %></td>
        <td><%= caixa.caixa_final %></td>
        <td><%= link_to 'Show', caixa %></td>
        <td><%= link_to 'Edit', edit_caixa_path(caixa) %></td>
        <td><%= link_to 'Destroy', caixa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Caixa', new_caixa_path %>
